{
  "expo": {
    "name": "SmartScribe",
    "slug": "SmartScribe",
    "version": "1.0.0",
    "icon": "./src/assets/logo1-smart.png",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seuapp.smartscribe",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Este aplicativo precisa de acesso ao microfone para gravar Ã¡udio.",
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "permissions": ["RECORD_AUDIO"],
      "package": "com.seuapp.smartscribe"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "routerRoot": "./src/app"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "753bb20e-0c35-4967-87fd-56478bcae8a2"
      }
    }
  },
  "cli": {
    "appVersionSource": "project"
  }
}
